{
	"name": "uwRMP",
	"description": "RMP extension for the University of Washington course page (MyPlan).",
	"version": "1.0.0",
	"manifest_version": 2,
	"icons": {
		"128": "assets/RMPuw.png"
	},
	"applications": {
		"gecko": {
      "id": "khazapar7@gmail.com",
			"strict_min_version": "42.0"
		}
	},
  "browser_specific_settings": {
    "gecko": {
      "id": "khazapar7@gmail.com",
      "strict_min_version": "42.0"
    }
  },  
	"permissions": ["activeTab", "webRequest", "webNavigation", "tabs", "https://myplan.uw.edu/course/*", "https://www.ratemyprofessors.com/*"],
	"content_scripts": [
		{
			"matches": ["https://myplan.uw.edu/course/*"],
			"js": ["content.js"]
		}
	],
	"background": {
		"scripts": ["background.js"]
	}
}
